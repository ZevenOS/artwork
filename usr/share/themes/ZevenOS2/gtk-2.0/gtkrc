     {
	    image
	    {
	     	function	= RESIZE_GRIP
		recolorable	= TRUE
		detail		= "statusbar"
		overlay_file	= "resize_grip.png"
		
		overlay_border	= {1,1,1,1 }
		overlay_stretch	= FALSE
	    }
      }
}
