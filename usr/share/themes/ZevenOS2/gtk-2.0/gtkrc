style "toolbuttons" = "default"
{
font_name = "Lucida Grande 8"
	xthickness = 0
	ythickness = 0

	GtkWidget::focus_padding = 2

	engine "pixmap" 
	{

		# Toolbar button
		image
		{
			function        	= BOX
			recolorable     	= TRUE
			state				= NORMAL
			file            = "Toolbar/toolbutton-normal.png"
	        border          = { 5, 5, 5, 5 }
			stretch         	= TRUE
		}
		
		# Toolbar button (mouse over)
		image
		{
			function      		= BOX
			recolorable    		= TRUE
			state				= PRELIGHT
			file            = "Toolbar/locationbar-hover.png"
	        border          = { 5, 5, 1, 1 }
			stretch       		= TRUE
		}

		# Toolbar button (clicked)
		image
		{
			function        	= BOX
			recolorable     	= TRUE
			state				= ACTIVE
			file            = "Toolbar/locationbar-hover.png"
	        border          = { 5, 5, 1, 1 }
			stretch         	= TRUE
		}  
		
		# Toolbar button (disabled)
		image
		{
			function      		= BOX
			recolorable    		= TRUE
			state				= INSENSITIVE
			file          		= "Toolbar/toolbutton-normal.png"
			border        		= { 5, 5, 5, 5 }
			stretch       		= TRUE
		}  
	}
}

