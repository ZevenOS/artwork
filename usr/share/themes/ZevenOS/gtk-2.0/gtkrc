style "list-header"
{
   #Comment out the ythickness setting below for thicker column headers.
   #ythickness = 0
   #GtkTreeView::odd_row_color = "#d3d3d3"
   #GtkTreeView::even_row_color = "#ffffff"
   #text[PRELIGHT] = "#FFFFFF"
  text[NORMAL]   = "#000000"
