	gap_end_file		= "null.png"
	gap_side		= TOP
      }
    image 
      {
        function			= BOX_GAP
	recolorable		= TRUE
	file				= "notebook.png"
	border			= { 2, 2, 2, 2 }
	stretch			= TRUE
	gap_file			= "gap_bottom.png"
	gap_border		= { 2, 2, 1, 1 }
	gap_start_file	= "null.png"
	gap_end_file		= "null.png"
	gap_side		= BOTTOM
      }
    image 
      {
        function			= BOX_GAP
	recolorable		= TRUE
	file				= "notebook.png"
	border			= { 2, 2, 2, 2 }
	stretch			= TRUE
	gap_file			= "gap_left.png"
	gap_border		= { 2, 2, 6, 2 }
	gap_start_file	= "null.png"
	gap_end_file		= "null.png"
	gap_side		= LEFT
      }
    image 
